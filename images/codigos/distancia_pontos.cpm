use "stdio";

def distancia_pontos(float x1, float y1, float x2, float y2) :: float {
    float dx = x2 - x1;
    float dy = y2 - y1;
    float distancia = sqrt(dx*dx + dy*dy);
    return distancia;
};

def main() :: int {
    float x1 = 0, y1 = 0, x2 = 0, y2 = 0;
    read(x1);
    read(y1);
    read(x2);
    read(y2);

    float distancia = 0.0;
    distancia = distancia_pontos(x1, y1, x2, y2);
    print(distancia);

    return 0;
};