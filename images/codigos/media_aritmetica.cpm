use "stdio";

def media_aritmetica(int a, int b, int c) :: float {
    float media = (a + b + c) / 3.0;
    return media;
};

def main() :: int {

    int a = 0, b = 0, c = 0;
    read(a);
    read(b);
    read(c);

    float media = 0.0;
    media = media_aritmetica(a, b, c);
    print(media);

    return 0;
};